<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 自定义edittext定义的属性-->
    <declare-styleable name="LiteEditText">

        <attr name="ledittext_ic_delete" format="reference" />
        <attr name="ledittext_delete_x" format="integer" />
        <attr name="ledittext_delete_y" format="integer" />
        <attr name="ledittext_delete_width" format="integer" />
        <attr name="ledittext_delete_height" format="integer" />

        <attr name="ledittext_ic_left_click" format="reference" />
        <attr name="ledittext_ic_left_unclick" format="reference" />
        <attr name="ledittext_left_x" format="integer" />
        <attr name="ledittext_left_y" format="integer" />
        <attr name="ledittext_left_width" format="integer" />
        <attr name="ledittext_left_height" format="integer" />

        <attr name="ledittext_lefticon" format="integer" />
        <attr name="ledittext_lineColor_click" format="color" />
        <attr name="ledittext_lineColor_unclick" format="color" />
        <attr name="ledittext_linePosition" format="integer" />

        <attr name="ledittext_cursor" format="reference" />


    </declare-styleable>


</resources>